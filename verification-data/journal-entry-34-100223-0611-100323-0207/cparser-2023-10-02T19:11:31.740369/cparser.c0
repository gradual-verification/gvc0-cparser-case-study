Token *new_token(TokenKind *kind, StringList *contents) {
  Token *tok = alloc(struct Token);
  tok->kind = kind;
  tok->str = contents;
  return tok;
}

bool equal(Token *tok, StringList *op)
//@ requires ? && tok != NULL;
//@ ensures ? && tok != NULL;
{
  return equals(tok->str, op);
}

Token *skip(Token *tok, StringList *op) 
//@ requires ? && tokenListSeg(tok,NULL);
//@ ensures ? && tokenListSeg(tok,\result) && tokenListSeg(\result,NULL) && tok != \result;
{
  if (!equal(tok, op)) {
    token_match_error();
    gv_tok_unreachable();
  }
  //@ unfold tokenListSeg(tok,NULL);
  Token *res = tok->next;
  //@ unfold tokenListSeg(res,NULL);
  //@ assert tok != res;
  //@ fold tokenListSeg(res,NULL);
  //@ fold tokenListSeg(res,res);
  //@ fold tokenListSeg(tok,res);
  return res;
}

bool consume(Token **rest, Token *tok, StringList *str) {
  if (equal(tok, str)) {
    *rest = tok->next;
    return true;
  }
  *rest = tok;
  return false;
}

StringList *tok_name(Token *tok) {
  if (tok == NULL) {
    return NULL;
  } else {
    return tok->str;
  }
}

