bool same_typekind(Type *ty1, Type *ty2) {
  if (ty1 == NULL       || ty2 == NULL ||
      ty1->kind == NULL || ty2->kind == NULL
     )
  {
    return false;
  } else {
    return ty1->kind->kind == ty2->kind->kind;
  }
}

Type *new_type(TypeKind *kind, int size, int align) {
  Type *ty = alloc(struct Type);
  ty->kind = kind;
  ty->size = size;
  ty->align = align;
  return ty;
}
